0	regex	@.*\n[ATGCNatgcn]*\n+.*\n[!-I]*\n	Fastq (Sanger)
!:mime	text/fastq
0	regex	@.*\n[ATGCNatgcn]*\n+.*\n[;-i]*\n	Fastq (Solexa)
!:mime	text/fastq
0	regex	@.*\n[ATGCNatgcn]*\n+.*\n[!-J]*\n	Fastq (Illumina 1.8)
!:mime	text/fastq
0	regex	@.*\n[ATGCNatgcn]*\n+.*\n[@-i]*\n	Fastq (Illumina 1.3)
!:mime	text/fastq
